pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        echo 'Checking out ...'
      }
    }

    stage('Build') {
      steps {
        echo 'Building ...'
      }
    }

    stage('Analyze') {
      steps {
        echo 'Analyzing ...'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploying ...'
      }
    }

  }
}